generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Orders {
  id        Int     @id  @default(autoincrement())
  plan  String  
  name  String
  email  String @unique
  phone  String @unique
  amount String
  status Boolean @default(false)
}

model Contact {
  id      Int     @id  @default(autoincrement())
  name  String  
  email  String @unique
  message String
}
